<div class="dialog-content apply-detail">
    <div class="col-containner">
        <div class="line-col">
            <span>登记编号:</span>
            <span><%= data.data.REGID %></span>
        </div>
        <div class="line-col">
            <span>登记人员:</span>
            <span><%= data.data.USERNAME %></span>
        </div>
        <div class="line-col">
            <span>登记时间:</span>
            <span> <%= data.data.REGDATE ? data.data.REGDATE.substr(0, 10) : "" %> </span>
        </div>

        <div class="line-col">
            <span>处理时间:</span>
            <span><%= data.data.HANDLEDATE ? data.data.HANDLEDATE.substr(0, 10) : "" %></span>
        </div>

        <div class="line-col">
            <span>处理人员:</span>
            <span><%= data.data.HANDLEUSERNAME %></span>
        </div>
        <div class="line-col">
            <span>处理状态:</span>
            <span><%= data.data.HANDLESTATUS == "2" ? "已处理" : "未处理" %></span>
        </div>

        <div class="line-col">
            <span>分类名称:</span>
            <span><%= data.data.CLSNAME %></span>
        </div>
        <div class="line-col">
            <span>检索关键字:</span>
            <span><%= data.data.KEYWORD %></span>
        </div>
		<div class="line-col">
            <span>问题来源:</span>
            <span><%= data.data.REQUIRENAME %></span>
        </div>

		
    </div>

