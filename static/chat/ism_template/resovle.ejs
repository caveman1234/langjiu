    <!--</div>-->

    <!--</div>-->

    <!--<% }) %>-->
    <!--</div>-->
    <div class="register-detail-filelist-1 noborder">
        <div class="check_file-containner">
            <span class="check_file">
                <img class="icon-down" src="/ism/chat/img/ism_img/down.png"> 查看附件
            </span>
        </div>

        <% data.attachment.forEach(function(data, index){ %>
        <div class="attachmentfile" id="<%= data.ATTACHMENTID %>">
            <% var arr = data.ATTACHMENTNAME.split(".");  if(arr[arr.length - 1] == "png"
                    || arr[arr.length - 1] == "jpg" || arr[arr.length - 1] == "gif"){ %>
            <img src="/ism/chat/img/ism_img/pic.png">
            <% }else{ %>
            <img src="/ism/chat/img/svg/file.svg">
            <% } %>
            <a href="<%= data.HTTPURL %>"><%= data.ATTACHMENTNAME %></a>
        </div>

        <% }) %>

    </div>

    <div class=" line-row">
        <span>处理方案:</span>
        <span id="resolve_plan" class="disabled-area changeline"><%= data.data.PROJECTCONTENT %></span>
    </div>

    <div>
        <div class="mc-mg">方案附件：</div>
        <div class="mc-contain">
            <% if(data.attachmentproject){
                    data.attachmentproject.forEach(function(data, index){ %>

            <div class="attachmentfile" id="<%= data.ATTACHMENTID %>">
                <% var arr = data.ATTACHMENTNAME.split(".");  if(arr[arr.length - 1] == "png"
                        || arr[arr.length - 1] == "jpg" || arr[arr.length - 1] == "gif"){ %>
                <img src="/ism/chat/img/ism_img/pic.png">
                <% }else{ %>
                <img src="/ism/chat/img/svg/file.svg">
                <% } %>
                <a href="<%= data.HTTPURL %>"><%= data.ATTACHMENTNAME %></a>
            </div>

            <% })} %>
        </div>

    </div>

</div>


<div class="dialog-footer">
    <%if(data.data.HANDLESTATUS == "2"){ %>
        <button id="quickreply">快速回复</button>
    <%}else{ %>
        <button class="mc-disable disabled_button " id="quickreply">快速回复</button>
    <%}%>
</div>









